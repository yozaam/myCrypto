ransaction.js","original":{"line":60,"column":50}},{"name":"conductTransaction","generated":{"line":117,"column":22},"source":"components/ConductTransaction.js","original":{"line":60,"column":55}},{"generated":{"line":118,"column":0},"source":"components/ConductTransaction.js","original":{"line":60,"column":16}},{"generated":{"line":118,"column":18},"source":"components/ConductTransaction.js","original":{"line":35,"column":12}},{"generated":{"line":118,"column":19},"source":"components/ConductTransaction.js","original":{"line":34,"column":8}},{"generated":{"line":119,"column":0},"source":"components/ConductTransaction.js","original":{"line":64,"column":5}},{"name":"Component","generated":{"line":123,"column":2},"source":"components/ConductTransaction.js","original":{"line":8,"column":33}},{"generated":{"line":125,"column":0},"source":"components/ConductTransaction.js","original":{"line":65,"column":1}},{"name":"ConductTransaction","generated":{"line":126,"column":15},"source":"components/ConductTransaction.js","original":{"line":67,"column":15}}],"sources":{"components/ConductTransaction.js":"import React, {Component} from 'react';\nimport {FormGroup, FormControl, Button} from 'react-bootstrap';\nimport history from '../history';\n\nimport {Link} from 'react-router-dom';\nimport hoops from \"../assets/hoops.gif\";\n\nclass ConductTransaction extends Component{\n    state = { recipient : \"\", amount : 0 };\n\n    updateRecipient = event => {\n        this.setState({recipient:event.target.value});\n    };\n\n    updateAmount = event => {\n        this.setState({amount:Number(event.target.value)});\n    };\n\n    conductTransaction = () => {\n        const { recipient , amount } = this.state;\n\n        fetch(`${document.location.origin}/api/transact`,{\n            method:'POST',\n            headers:{ 'Content-Type':'application/json'},\n            body: JSON.stringify({recipient,amount})\n        }).then(response => response.json())\n            .then(json => {\n                alert(json.message || json.type);\n                history.push('/transaction-pool');\n            });\n    };\n\n    render(){\n        return (\n            <div className='ConductTransaction'>\n                <img className=\"logo\" src={hoops} alt=\"...\"/>\n                <br/>\n                <br/>\n                <Link to='/'>Home</Link>\n                <h3>Conduct a Transaction</h3>\n                <br />\n\n                <FormGroup>\n                    <FormControl\n                        input='text'\n                        placeholder='recipient'\n                        value={this.state.recipient}\n                        onChange={this.updateRecipient}\n                    />\n                </FormGroup>\n                <FormGroup>\n                    <FormControl\n                        input='number'\n                        placeholder='amount'\n                        value={this.state.amount}\n                        onChange={this.updateAmount}\n                    />\n                </FormGroup>\n\n                <Button variant=\"danger\" onClick={this.conductTransaction}>Submit</Button>\n\n            </div>\n        );\n    }\n};\n\nexport default ConductTransaction;"},"lineCount":null}},"error":null,"hash":"ce32ba331a2f4707b66c69b064498660","cacheData":{"env":{}}}